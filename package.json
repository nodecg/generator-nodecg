{
  "name": "generator-nodecg",
  "version": "1.2.0",
  "description": "Yeoman generator for NodeCG bundles",
  "homepage": "http://nodecg.dev",
  "author": {
    "name": "Alex Van Camp",
    "email": "email@alexvan.camp",
    "url": "http://alexvan.camp"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman-generator",
    "nodecg",
    "bundle"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "deep-extend": "^0.6.0",
    "generator-license": "^5.0.0",
    "generator-node": "^2.7.0",
    "git-config": "0.0.7",
    "github-username": "^6.0.0",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "parse-author": "^2.0.0",
    "yeoman-environment": "^3.12.1",
    "yeoman-generator": "^5.7.0",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "@types/deep-extend": "^0.6.0",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.190",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^10.0.0",
    "@types/mockery": "^1.4.30",
    "@types/node": "^18.11.9",
    "@types/parse-author": "^2.0.1",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^5.2.11",
    "@types/yosay": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "app-root-path": "^3.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.55.0",
    "fs-extra": "^10.1.0",
    "mocha": "^10.1.0",
    "mockery": "^2.1.0",
    "npm-name": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "onchange": "^7.1.0",
    "prettier": "^2.8.0",
    "standard-version": "^9.5.0",
    "trash-cli": "^5.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.2.0",
    "typescript": "^4.9.3",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodecg/generator-nodecg.git"
  },
  "scripts": {
    "lint": "npm-run-all -s lint:*",
    "lint:prettier": "prettier --check \"**/*.{json,md,yml,ts,tsx,js,jsx}\"",
    "lint:eslint": "eslint \"**/*.{js,ts}\"",
    "fix": "npm-run-all -s fix:*",
    "fix:prettier": "prettier --write \"**/*.{json,md,yml,ts,tsx,js,jsx}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "test": "nyc --reporter=lcov ts-mocha -r source-map-support/register test/**/*.ts",
    "watch": "onchange --initial generators/**/*.js test/** -- nyc ts-mocha -r source-map-support/register --type-check test/**/*.ts",
    "report-coverage": "codecov",
    "prerelease": "npm run lint && npm t",
    "release": "standard-version",
    "postrelease": "npm publish && git push --follow-tags",
    "prebuild": "trash generators",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "node ./scripts/copy-templates.js"
  },
  "license": "MIT"
}
