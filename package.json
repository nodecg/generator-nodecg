{
  "name": "generator-nodecg",
  "version": "1.1.0",
  "description": "Yeoman generator for NodeCG bundles",
  "homepage": "http://nodecg.com",
  "author": {
    "name": "Alex Van Camp",
    "email": "email@alexvan.camp",
    "url": "http://alexvan.camp"
  },
  "files": [
    "generators"
  ],
  "main": "generators\\index.js",
  "keywords": [
    "yeoman-generator",
    "nodecg",
    "bundle"
  ],
  "dependencies": {
    "chalk": "^2.0.0",
    "deep-extend": "^0.5.0",
    "generator-license": "^5.0.0",
    "generator-node": "^2.7.0",
    "git-config": "0.0.7",
    "github-username": "^4.1.0",
    "lodash": "^4.17.15",
    "mkdirp": "^0.5.1",
    "parse-author": "^2.0.0",
    "yeoman-generator": "^3.0.0",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-xo": "^0.23.0",
    "mocha": "^5.2.0",
    "mockery": "^2.1.0",
    "npm-name": "^3.2.0",
    "nyc": "^13.0.1",
    "onchange": "^5.0.2",
    "standard-version": "^8.0.1",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.9.1"
  },
  "resolutions": {
    "natives": "1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodecg/generator-nodecg.git"
  },
  "scripts": {
    "static": "eslint **/*.js",
    "test": "npm run static && nyc --reporter=lcov mocha",
    "watch": "onchange --initial generators/**/*.js test/** -- nyc mocha",
    "report-coverage": "codecov",
    "prerelease": "npm t",
    "release": "standard-version",
    "postrelease": "npm publish && git push --follow-tags"
  },
  "license": "MIT"
}
